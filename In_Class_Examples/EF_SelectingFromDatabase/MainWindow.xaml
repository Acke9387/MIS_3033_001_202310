<Window x:Class="EF_SelectingFromDatabase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EF_SelectingFromDatabase"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstAway" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="450,0,0,0"/>
        <Button x:Name="btnGetData" Content="Get Data" HorizontalAlignment="Left" Margin="10,148,0,0" VerticalAlignment="Top" Height="41" Width="63" Click="btnGetData_Click"/>
        <ListBox x:Name="lstHome" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="98,0,352,0"/>

    </Grid>
</Window>
